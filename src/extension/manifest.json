{
  "description": "Some useful functionality for Utkonos.ru the Web Supermarket",
  "manifest_version": 2,
  "name": "Utkonos Assistant",
  "version": "*",
  "homepage_url": "https://github.com/alexey-lapin/utkonos-assistant#readme",
  "icons": {
    "16": "img/icon.svg",
    "48": "img/icon.svg",
    "128": "img/icon.svg"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.utkonos.ru/*"],
      "js": ["inject/index.js"]
    }
  ]
}
